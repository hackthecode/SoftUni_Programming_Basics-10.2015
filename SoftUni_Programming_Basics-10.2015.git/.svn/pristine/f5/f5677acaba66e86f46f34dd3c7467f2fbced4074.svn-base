using System;

namespace _09.SumOfNnumbers
{
    class SumOfNnumbers
    {
        static void Main()
        {
            Console.Write("Enter integer number n: ");
            int n = int.Parse(Console.ReadLine());
            double sum = 0;

            for (int i = 0; i < n; i++)
            {
                double number = double.Parse(Console.ReadLine());
                sum += number;
            }
            Console.WriteLine("sum: {0}", sum);
        }
    }
}
